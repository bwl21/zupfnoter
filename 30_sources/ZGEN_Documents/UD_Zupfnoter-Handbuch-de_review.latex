\end{verbatim}

\hypertarget{extract.0.repeatsigns.left.text}{%
\subsection{\texorpdfstring{\texttt{extract.0.repeatsigns.left.text} -
Text}{extract.0.repeatsigns.left.text - Text}}\label{extract.0.repeatsigns.left.text}}

