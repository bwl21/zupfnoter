        "imagename" : "",
        "pos"       : [10, 10],
        "show"      : true
      },
      "lyrics"      : {"pos": [350, 70], "style": "regular", "verses": [1]},
      "notes"       : {"pos": [320, 6], "style": "large", "text": "ENTER_NOTE"},
      "tuplet"      : {
        "cp1"   : [5, 2],
